
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option upd_privateip '0'

config service 'oray'
	option service_name 'oray.com'
	option enabled '1'
	option lookup_host 'weibs.gicp.net'
	option domain 'weibs.gicp.net'
	option username 'weibs'
	option password 'yuquan1978'
	option ip_source 'interface'
	option interface 'vwan1'
	option ip_interface 'pppoe-vwan1'

