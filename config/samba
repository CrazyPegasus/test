
config samba
	option name 'Lede'
	option workgroup 'WORKGROUP'
	option description 'Lede'
	option homes '4'

config sambashare
	option name 'disk1'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '0777'
	option dir_mask '0777'
	option path '/tmp/mnt/sda1'

config sambashare
	option read_only 'no'
	option name 'disk2'
	option guest_ok 'yes'
	option create_mask '0777'
	option dir_mask '0777'
	option path '/tmp/mnt/sda2'

config sambashare
	option read_only 'no'
	option name 'disk3'
	option guest_ok 'yes'
	option create_mask '0777'
	option dir_mask '0777'
	option path '/tmp/mnt/sda3'

config sambashare
	option name 'disk4'
	option read_only 'no'
	option guest_ok 'yes'
	option create_mask '0777'
	option dir_mask '0777'
	option path '/tmp/mnt/sda4'

